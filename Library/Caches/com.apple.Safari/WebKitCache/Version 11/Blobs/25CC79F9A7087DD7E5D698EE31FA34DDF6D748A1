    (function () {
        var hash = window.location.hash.substr(1);

        if (hash) {
            var regExBsonId = /^[0-9A-F]{24}$/i;
            var regExInt = /^\d+$/;
            var isBsonId = hash.match(regExBsonId);
            var isInt = hash.match(regExInt);

            if (isBsonId || isInt) {
                var redirectUrl = window.location.pathname + '/' + hash;

                if (isInt) {
                    redirectUrl += '?ref=' + document.referrer;
                }

                window.location = redirectUrl;
            }
        }
    })();
