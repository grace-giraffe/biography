            <script>
        (function() {
            if (location.pathname !== "/error/upgradebrowser") {
                var n = navigator.appName, ua = navigator.userAgent, tem;
                var m = ua.match(/(opera|chrome|safari|firefox|msie)\/?\s*(\.?\d+(\.\d+)*)/i);
                if (m && (tem = ua.match(/version\/([\.\d]+)/i)) != null) m[2] = tem[1];
                var browserDetails = m ? [m[1], m[2]] : [n, navigator.appVersion, '-?'];
                var browserDetailsName = browserDetails[0];
                var browserDetailsVersion = parseInt(browserDetails[1], 10);
                var redirectUrl = "/error/upgradebrowser?status=browser";
                var isAndroid = ua.match(/(android)\/?\s*(\.?\d+(\.\d+)*)/i);

                if ((browserDetailsName == "MSIE" && browserDetailsVersion <= 10) ||
                    (browserDetailsName == "Firefox" && browserDetailsVersion <= 3.6) ||
                    (browserDetailsName == "Opera" && browserDetailsVersion <= 10.6) ||
                    (browserDetailsName == "Safari" && browserDetailsVersion <= 9 && !isAndroid)) {
                    var trackingData = {
                        userAgent: ua,
                        referringPath: document.location.pathname
                    };
                    window.analytics.track('UnsupportedBrowserDetected', trackingData);
                    // We set a short time out before redirecting
                    // to ensure that the analytics.track() request gets time to finish
                    setTimeout(function() { window.location = redirectUrl; }, 200);
                }
            }
        })();
    </script>


        
