    <script type="text/javascript" src="js/greystonePM_listings.js"></script>
    <script type="text/javascript">
        var popup_bg = $("#popup_bg");
        var popup = $("#popup");
        popup_bg.hide();
        popup.hide();

        var popUpHeader_results = $("#popUpHeaderDiv");
        var popUpDiv_results = $("#popUpDiv");
        function openPopup(strProperty, strRenderMe) {

            //  toggle_visibility('galleryDiv');

            var body = "";
            var header = "";
            var count = 0;

            body += "";

            for (var i = 0; i < offices_length; i++) {
                var obj = offices[i];

                if (obj.c == strProperty) {

                    header += "<h1>" + obj.a + "</h1>\n";
                    switch (strRenderMe) {
                        case "Amenities":

                            var apartmentAmmenities = "";
                            if (obj.p != undefined && obj.p.length > 0) {
                                var index = fieldIndex(obj.p, "k", "");

                                if (index > -1) {
                                    apartmentAmmenities = "<ul>\n";

                                    for (var j = 0; j < obj.p[index].p.length; j++) {
                                        var p = obj.p[index].p[j];
                                        apartmentAmmenities += "<li>\n<span class=\"txt\">" + p.n + "</span></li>\n";
                                    }
                                    apartmentAmmenities += "</ul>\n";
                                }
                            }


                            var communityAmmenities = "";
                            if (obj.q != undefined && obj.q.length > 0) {
                                var index = fieldIndex(obj.q, "k", "");

                                if (index > -1) {
                                    communityAmmenities = "<ul>\n";

                                    for (var j = 0; j < obj.q[index].p.length; j++) {
                                        var p = obj.q[index].p[j];
                                        communityAmmenities += "<li>\n<span class=\"txt\">" + p.n + "</span></li>\n";
                                    }
                                    communityAmmenities += "</ul>\n";
                                }
                            }

                            body += "<div class=\"clear full\">\n" +
                                    "<div class=\"half floatleft\">\n" +
                                    "<h2><span class=\"green\">Apartment Amenities</span></h2><br /><br />\n" +
                                    apartmentAmmenities +
                                    "</div>\n" +
                                    "<div class=\"half floatright\">\n" +
                                    "<h2><span class=\"green\">Community Amenities</span></h2><br /><br />\n" +
                                    communityAmmenities +
                                    "</div>\n" +
                                    "</div>\n" +
                                    "<div class=\"clear full border\"></div>\n";

                            count++;
                            break;
                        case "SlideShow":

                            break;
                        case "FloorPlans":
                            var apartmentFloorplans = "";
                            var functionText = "";
                            if (obj.t != undefined && obj.t.length > 0) {
                                var index = fieldIndex(obj.t, "k", "");

                                if (index > -1) {
                                    apartmentAmmenities = "<ul>\n";

                                    for (var j = 0; j < obj.t[index].p.length; j++) {
                                        var p = obj.t[index].p[j];
                                        apartmentFloorplans += "<li" +
                                                                " onmouseover=\"javascript:showFloorplan('" +
                                                                p.n + "','" + p.i + "','" + p.d + "');\"" +
                                                                ">\n<a><span style=\"cursor:pointer\">" + p.n + "</span></a></li>\n";
                                    }
                                    apartmentFloorplans += "</ul>\n";
                                }
                            }

                            body += "<div class=\"floatleft\">\n" +
                                    "<h2><span class=\"green\">Apartment Floorplans</span></h2><span><p> - Hover over unit type to view floorplan.</p></span><br /><br />\n" +
                                        apartmentFloorplans +
                                        "</div>\n";

                            count++;
                            $("#popUpContent").height('625');
                            break;
                        default:
                    }
                }

                popUpHeader_results.html(header);
                popUpDiv_results.html(body);

                if (count > 0) {
                    $("ul", popUpDiv_results).hide().fadeIn("slow");
                }

            }

            if (count == 0) {
                body = "<div class=\"copy full\">No office data found.<br /><br /><a href=\"rentalLocations.html\">View all offices</a></div>\n";
            }

            popup_bg.stop().fadeTo("slow", 0.2);
            popup.stop().fadeTo("slow", 1);
        }


        function closePopup() {
            popup_bg.stop().fadeTo("fast", 0, function () { $(this).hide(); });
            popup.stop().fadeTo("fast", 0, function () { $(this).hide(); });
            $("#divPopupVideo").html('');
            $("#popUpHeaderDiv").html('');
            $("#popUpDiv").html('');
            $("#subnav_displayMe").html('');
            // toggle_visibility('galleryDiv');
            $("#popUpContent").height('550');
        }

        if (ua_ie7) {	// fix for IE7 overlap issue
            loadcomplete_functions.push(function () {
                popup_bg.remove();
                popup.remove();
                $("#main").append(popup_bg).append(popup);
            });
        }

        function toggle_visibility(id) {
            var e = document.getElementById(id);
            if (e.style.display == 'block')
                e.style.display = 'none';
            else
                e.style.display = 'block';
        }

        function showFloorplan(unitType, unitImageURL, unitDescription) {
            var floorPlan = ""
            var popUpDisplayMe = $("#subnav_displayMe");


            floorPlan += "<div class=\"floatrightFloorPlan\">\n" +
                            "<h2 style=\"margin-left: 30px;\"><span class=\"green\">" + unitType + " : </span><span class=\"grey\">" + unitDescription + "</span></h2><br /><br />\n" +
                            "<img src=\"" + unitImageURL + "\"></img><br /><br />\n" +
                            "</div>\n";

            popUpDisplayMe.html(floorPlan);

        }

    </script>

