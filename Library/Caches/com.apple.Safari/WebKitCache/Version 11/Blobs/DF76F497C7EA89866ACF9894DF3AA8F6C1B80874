<script type="text/javascript">
try {
	if (HRBData.digital_data.dtmEnabled){
		if(!HRBData.digital_data.satellitePageBottomHasFired){
			// During DTM transition period, pageBottom() might try to fire 
			// in hrb.js, omniture_std.html AND common-bottom.html.  Until pages
			// include all three files, each file must attempt pageBottom().
			HRBData.digital_data.satellitePageBottomHasFired=true;
			console.log("DTM:common-bottom.html: calling _satellite.pageBottom()");
			_satellite.pageBottom();
			console.log("DTM:common-bottom.html: _satellite.publishDate | buildDate : "+_satellite.publishDate+" | "+_satellite.buildDate);
		}
	}
}
catch(e)
{
	console.log("DTM:common-bottom.html: error calling _satellite.pageBottom():"+e);
}
</script>
<!-- end: common-bottom.html -->

<script>function init() {
    /* add class to <html> to let the CSS know it's okay to load mobile background images */
    $('html').addClass('page-loaded');

    /* defer image load */
    $('img').each(function(i){
        var thisImg = $(this);
        if( thisImg.data('src') !== undefined ){
            var thisImgData = thisImg.data('src');
            thisImg.attr('src', thisImgData);
        }
    });

}

$(window).on('load', function(){
    init();
});



$(document).ready(function(){
    /* poster image with embedded youtube video */
    $(document).on('click touchstart', '.play-btn, .video-wrapper .poster', function(e){
        e.stopPropagation();
        e.preventDefault();
        if(e.type == 'click' || e.type == 'touchstart'){
            var elm  = $(this).parents('.video-wrapper'),
                conts   = elm.contents(),
                le      = conts.length,
                ifr     = null;
            for(var i = 0; i<le; i++){
              /* nodeType 8 = comments */
              if(conts[i].nodeType == 8) ifr = conts[i].textContent;
            }
            elm.addClass('player').html(ifr);
            elm.off('click touchstart');
            
            // in case video-bg is used and its siblings need to know video is loaded
            $('.video-bg').addClass('player');
        }
    });

    /*
        Show only Financial Services disclaimers on pages in
        /financial-services/ and /lp/financial-services/
    */
    var fsPagePath = document.location.pathname;
    if ( fsPagePath.match( '\/financial-services\/' ) ){
        $('body').addClass('jsShowFinancialDisclaimers');
    }
    
    var showFinancialDisclaimers = $('body').hasClass('jsShowFinancialDisclaimers');
    if (showFinancialDisclaimers) {
        $('.js-disclaimer-list > p, .js-disclaimer-list div[id^="disc"]').css('display', 'none');
        $('#discFinancialServicesLink').parent('p').removeAttr('style');
        $('#discFinancialServicesList').removeAttr('style');
    }
    
    /* accordions */
    $('.accordion__btn').on('click', function(){
        var $button = $(this);
        var isOpen = $button.attr('aria-expanded');
        var ariaControls = $button.attr('aria-controls');
        var $thisContent = $('#' + ariaControls);

        if(isOpen=="true"){
            $button.attr('aria-expanded', 'false');
            $thisContent.slideUp();
        } else {
            //$('.accordion__btn').attr('aria-expanded', 'false');
            //$('.accordion__content').slideUp();
            $button.attr('aria-expanded', 'true');
            $thisContent.slideDown();
        }
    });
});</script>

