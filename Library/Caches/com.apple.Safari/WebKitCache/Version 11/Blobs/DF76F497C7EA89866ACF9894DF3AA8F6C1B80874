<footer id="site-footer">
    <div class="footer-container">
        <div class="footer-disclaimers">
            <!-- BEGIN: disclaimers.html -->
<style>
/*  legacy page only */
    .content #legal-disclaimer { margin: 20px;}
    .content #legal-disclaimer button { background: none; border: none; color: #276702; font-weight: bold; display: block; margin-bottom: 10px; padding: 0;}
    .content #legal-disclaimer button:hover,
    .content #legal-disclaimer button:focus { color: #276702; text-decoration: none; cursor: pointer; outline: none;}
    .content #legal-disclaimer button:focus { text-decoration: underline;}
    .content #legal-disclaimer button [class*="icon-"] { text-decoration: none!important;}
    .content #legal-disclaimer ol { font-size: 1.4em; }
    .content #legal-disclaimer ol li { padding: 0 0 10px;}

    /* top level */
    .content #disclaimer-list { margin-left: 10px; display: none;}
    .content #disclaimer-link[aria-expanded=true] + #disclaimer-list { display: block;}

    /* secondary level */
    .content #legal-disclaimer ol { list-style-type: decimal; display: none;}
    .content #legal-disclaimer button[aria-expanded=true] + ol { display: block;}

    /* show active disclaimer */
