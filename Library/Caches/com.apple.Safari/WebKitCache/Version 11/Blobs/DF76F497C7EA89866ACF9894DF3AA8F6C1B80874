	<script>
    $(document).ready(function() {

    /* Detect double tap zoom on mobile devices
       and prevent the top nav from displaying badly */
    $(window).on('touchstart', function(e){
        var t2 = e.timeStamp,
            t1 = $(this).data('lastTouch') || t2,
            dt = t2 - t1,
            fingers = e.originalEvent.touches.length;
        $(this).data('lastTouch', t2);
        if (!dt || dt > 500 || fingers > 1) {return;} // not double-tap
        
        setTimeout(function(){
            zoom = document.documentElement.clientWidth / window.innerWidth;
            //alert('zoom: ' + zoom);

            if (zoom > 1) {
                /* SWITCH NAV POSITIONING to absolute for touch devices */
                $('header.nav-bar').css('position', 'absolute');
            } else if (zoom <= 1) {
                /* and remove it when it goes back to 100% */
                $('header.nav-bar').removeAttr('style');
            }
        }, 200);
    });

    /* Also try to prevent a previously zoomed page from appearing ugly on refresh */
    setTimeout(function(){
        zoom = document.documentElement.clientWidth / window.innerWidth;
        /* SWITCH NAV POSITIONING to absolute for touch devices */
        $('header.nav-bar').css('position', 'absolute');
        if (zoom <= 1) {
            /* and remove it when it goes back to 100% */
            $('header.nav-bar').removeAttr('style');
        }
    }, 1200);


    /* CREATE MOBILE NAV FROM DESKTOP NAV */
    $('#desktop-nav').clone().prependTo('body')
        .attr({
            "id": "mobile-nav",
            "aria-hidden": true
        });

    /* Copy the aria attributes from the reader-open-submenu button
        to nav-link-products and nav-link-tools,
        then remove the button because it's not needed in the mobile nav */
    $('#mobile-nav [class*="nav-link-"]').each(function(i, el){
        var $thisLink = $(this);
        var $readerBtn = $thisLink.siblings('.reader-open-submenu');
        var ariaControls = $readerBtn.attr('aria-controls');
        $thisLink.attr({
            "aria-controls": ariaControls,
            "aria-expanded": "false"
        });
        $readerBtn.remove();
    });


    /* Add class wrapper to format mobile nav links */
    $('#mobile-nav li > a').wrapInner('<span class="vert-center"></span>');

    $('#mobile-nav [class*="nav-link-"], #mobile-nav button').each(function(i, el) {
        var ariaControlsId = $(this).attr('aria-controls');
        if (ariaControlsId) {
            $(this).attr('aria-controls', ariaControlsId + '-m');
            $('#' + ariaControlsId).attr('id', ariaControlsId + '-m');
        }
    });
    /* Expand mobile submenu on load if page is in that section */
    var mobilePagePath = document.location.pathname;
    /* Products */
    if ( mobilePagePath.match( 'filing-options-and-products|\/tax-offices\/|\/online-tax-filing\/|\/tax-software\/|\/financial-services\/' ) ){
        $('body').addClass('jsShowSubnavProducts');
    }
    /* Tools */
    if ( mobilePagePath.match( 'tax-center|\/get-answers\/|\/taxes\/|\/tax-offices\/tax-prep\/|\/tax-calculator\/|\/w-4-calculator\/|\/tax-prep-checklist\/' ) ){
        /* /tax-offices/tax-prep/ pages will also get "products" class above. Remove it. */
        $('body').removeClass('jsShowSubnavProducts').addClass('jsShowSubnavTools');
    }
    
    var showProducts = $('body').is('.homepage, .jsShowSubnavProducts');
    var showTools = $('body').is('.jsShowSubnavTools');
    if (showProducts) {
        $('#mobile-nav .nav-li-products .submenu-wrap').css('display', 'block');
        $('#mobile-nav .nav-link-products').attr('aria-expanded', true);
    }
    if (showTools) {
        $('#mobile-nav .nav-li-tools .submenu-wrap').css('display', 'block');
        $('#mobile-nav .nav-link-tools').attr('aria-expanded', true);
    }

    /* When mobile nav is visible, hide desktop nav from screen readers */
    var isMobileNav = Modernizr.mq('(max-width: 60.9375em)');
    if (isMobileNav) {
        $('#desktop-nav').attr('aria-hidden', true);
    }


    /* Mobile open/close button */
    $('.mobile-nav-btn').on('click', function() {
        if ($('html').hasClass('mobile-nav-open')) {
            closeMobileNav();
        } else {
            openMobileNav();
        }
    });

    /* Close desktop or mobile nav when clicking outside */
    $(document).on('touchstart click', function(e) {
        if (e.handled !== true) {
            var header = $('header.nav-bar');
            var mobileNav = $('#mobile-nav');
            var autocomplete = $('ul.ui-autocomplete');
            if (header.has(e.target).length === 0 &&
                mobileNav.has(e.target).length === 0 &&
                autocomplete.has(e.target).length === 0
            ) {
                closeMobileNav();
                closeDesktopNav();
            }
            e.handled = true;
        } else {
            return false;
        }
    });


    /*
        Close mobile nav when orientation changes.
        Switch the main nav to absolute position when the page is zoomed.
    */
    var resizeTimer;
    var windowWidth = $(window).width();
    var zoom = 1;

    $(window).resize(function() {
        clearTimeout(resizeTimer);

        resizeTimer = setTimeout(function() {
            if (windowWidth !== $(window).width()) {
                windowWidth = $(window).width();
                closeMobileNav();
            }

            /* hide/show desktop nav for screen readers */
            var isMobileNav = Modernizr.mq('(max-width: 60.9375em)');
            if (isMobileNav) {
                $('#desktop-nav').attr('aria-hidden', true);
            } else {
                $('#desktop-nav').attr('aria-hidden', false);
            }
        }, 100);

        /* Prevent the fixed position nav from blocking the screen when the page is zoomed on touch devices */
        zoom = document.documentElement.clientWidth / window.innerWidth;
        //alert('zoom: ' + zoom);

        if (zoom > 1) {
             //alert('zoom to absolute: ' + zoom);

            /* Fist HIDE the nav on zoomed touch devices that are larger. Small/mobile nav seems to work fine, but tablets don't redraw it well enough when switching between fixed/absolute positioning, so give it a delay */
            if(!isMobileNav){
                $('header.nav-bar').css('display', 'none');
                setTimeout(function(){
                    $('header.nav-bar').css('position', 'absolute');
                    $('header.nav-bar').css('display', 'block');
                }, 250);
            } else {
            /* SWITCH POSITIONING to absolute for touch devices */
                $('header.nav-bar').css('position', 'absolute');
            }
            
        } else if (zoom <= 1) {
            //alert('zoom to reset: ' + zoom);
            $('header.nav-bar').removeAttr('style');
        }

    });


    /* Open/close desktop link submenus - on hover */
    var hoverTimer;

    /* don't let the submenu link touch event bubble up, or the link won't work */
    $('#desktop-nav #nav-products-submenu a, #desktop-nav #nav-tools-submenu a').on('touchstart', function(e){
        e.stopImmediatePropagation();
    });

    $('#desktop-nav [class*="nav-link-"]').closest('li').on({
        mouseenter: function() {
            clearTimeout(hoverTimer);

            var $thisLi = $(this);
            var $readerBtn = $thisLi.find('.reader-open-submenu');
            var isOpen = $readerBtn.attr('aria-expanded');

            hoverTimer = setTimeout(function() {
                if (isOpen === 'false') {
                    // close others first
                    closeDesktopNav();

                    // open this menu
                    $thisLi.find('.submenu-wrap').slideDown();
                    $readerBtn.attr('aria-expanded', true);
                }
            }, 250);
        },
        mouseleave: function() {
            clearTimeout(hoverTimer);

            var $thisLi = $(this);
            var $readerBtn = $thisLi.find('.reader-open-submenu');
            var isOpen = $readerBtn.attr('aria-expanded');

            hoverTimer = setTimeout(function() {
                if (isOpen === 'true') {
                    $thisLi.find('.submenu-wrap').slideUp();
                    $readerBtn.attr('aria-expanded', false);
                }
            }, 750);
        },
        touchstart: function(e){
            e.preventDefault();

            var $thisLi = $(this);
            var $readerBtn = $thisLi.find('.reader-open-submenu');
            var isOpen = $readerBtn.attr('aria-expanded');

            if(isOpen === 'true'){
                var navLinkHref = $(this).find('[class*="nav-link-"]').attr('href');
                window.location = navLinkHref;
            } else {
                // close others first
                closeDesktopNav();

                // open this menu
                $thisLi.find('.submenu-wrap').slideDown();
                $readerBtn.attr('aria-expanded', true);
            }
        }
    });

    /* Style top nav link when the hidden reader button is focused */
    $('#desktop-nav .reader-open-submenu').on({
        focus: function(){
            $(this).closest('li').addClass('reader-open-submenu-focused');
        },
        blur: function(){
            $(this).closest('li').removeClass('reader-open-submenu-focused');
        }
    });
    /* Open/close desktop submenus on reader button click */
    $('#desktop-nav .reader-open-submenu').on('click', function(){
        clearTimeout(hoverTimer);

        var $readerBtn = $(this);
        var $thisLi = $readerBtn.closest('li');
        var isOpen = $readerBtn.attr('aria-expanded');

        if (isOpen === 'false') {
            // close others first
            closeDesktopNav();

            // open this menu
            $thisLi.find('.submenu-wrap').slideDown();
            $readerBtn.attr('aria-expanded', true);
        } else {
            // close this menu
            $thisLi.find('.submenu-wrap').slideUp();
            $readerBtn.attr('aria-expanded', false);
        }
    });

    /* Open/close button submenus - on CLICK */
    $('[class*="nav-btn-"]').on('click', function(e) {
        e.preventDefault();
        var $thisBtn = $(this);
        var submenuId = $thisBtn.attr('aria-controls');
        var isOpen = $thisBtn.attr('aria-expanded');

        if (isOpen === 'true') {
            $('#' + submenuId).closest('.submenu-wrap').slideUp();
            $thisBtn.attr('aria-expanded', false);
        } else {
            /* close others first */
            closeDesktopNav();

            /* open this one */
            $('#' + submenuId).closest('.submenu-wrap').slideDown();
            $thisBtn.attr('aria-expanded', true);
        }
    });

    /* Close desktop hover submenus - on FOCUS */
    $('.nav-bar .logo-block, .nav-bar [class*="nav-btn-"]').on('focus', function() {
        var $desktopNav = $('#desktop-nav');
        $desktopNav.find('.nav-li-products .submenu-wrap, .nav-li-tools .submenu-wrap').slideUp();
        $desktopNav.find('.reader-open-submenu').attr('aria-expanded', false);
    });

    /* Auto-complete search */
    try {
        $(appendedInputButton.inputElement).AdobeAutocomplete(appendedInputButton); // non-mobile form
    } catch (e) {
        HRB.tools.log("error: auto-complete for #appendedInputButton: " + e);
    }
    try {
        $(appendedInputButtonMobile.inputElement).AdobeAutocomplete(appendedInputButtonMobile); // mobile form
    } catch (e) {
        HRB.tools.log("error: auto-complete for #appendedInputButtonMobile " + e);
    }

});

function openMobileNav() {
    $('html').addClass('mobile-nav-open');
    /* if there is a sticky nav on the page, close it when the mobile nav opens */
    $('body').removeClass('show-sticky');
    $('#mobile-nav').attr('aria-hidden', false);
    $('.mobile-nav-btn').attr('aria-expanded', true);

    /* close any submenus that might be open */
    closeDesktopNav();
}

function closeMobileNav() {
    $('html').removeClass('mobile-nav-open');
    $('#mobile-nav').attr('aria-hidden', true);
    $('.mobile-nav-btn').attr('aria-expanded', false);
}

function closeDesktopNav() {
    $('.nav-bar .submenu-wrap').slideUp();
    $('.nav-bar [class*="nav-btn-"], .nav-bar .reader-open-submenu').attr('aria-expanded', false);
}

var appendedInputButton = {
    account: "sp1004c6a7",
    searchDomain: "http://sp1004c6a7.guided.ss-omtrdc.net/",
    inputElement: "input#appendedInputButton",
    inputFormElement: "form#searchForm1",
    delay: 300,
    minLength: 3,
    maxResults: 10,
    browserAutocomplete: false,
    queryCaseSensitive: false,
    startsWith: false,
    searchOnSelect: true,
    header: "",
    footer: ""
};
