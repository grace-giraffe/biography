<script type="text/javascript">
HrbTunneled={};

// **************************
// TEXT ABOVE THE BUTTON		 - default and partner specific values
// **************************
/* Possible values, depending on if coupons are enabled or not and if overrides for a partner are in effect
	- "Congratulations! You qualify for a special discount!"
	- "Thank you for being our partner."
	- "Second Look"+String.fromCharCode(174)+" Review: We Find Money Others Miss"
	EXAMPLE:
		HrbTunneled.buttonTitle	= {"default":"Thank you for being our partner.","pw_mcm_9423_7764":"Congratulations! You qualify for a special discount!"};
*/
HrbTunneled.buttonTitle				= {
"xdefault":				"Second Look<sup>"+String.fromCharCode(174)+"</sup> Review:<br class='br'> We Find Money Others Miss",
"default":				"Congratulations! You qualify for a special discount!",
"pw_mcm_9423_7764":		"Second Look<sup>"+String.fromCharCode(174)+"</sup> Review: <br class='br'> We Find Money Others Miss",
"123AA11AA22AA33AA44AA55":	"Second Look<sup>"+String.fromCharCode(174)+"</sup> Review: <br class='br'> We Find Money Others Miss"
};

// **************************
// TEXT ON THE BUTTON			- default and partner specific values
// **************************
/* Possible values, depending on if coupons are enabled or not and if overrides for a partner are in effect
	- "Print my Coupon"
	- "Get a Second Look"+String.fromCharCode(174)+" Review",
	EXAMPLE:
		HrbTunneled.buttonText	= {"default":"Get a Second Look"+String.fromCharCode(174)+" Review","pw_mcm_9423_7764":"Print my Coupon"};
*/
HrbTunneled.buttonText				= {
"xdefault":				"Get a Second Look"+String.fromCharCode(174)+" Review",
"default":				"Print my Coupon",
"pw_mcm_9423_7764":		"Get a Second Look"+String.fromCharCode(174)+" Review",
"123AA11AA22AA33AA44AA55":	"Get a Second Look"+String.fromCharCode(174)+" Review"
};

// **************************
// BUTTON ACTION/DESTINATION	- default and partner specific values
// **************************
/* Possible values, depending on if coupons are enabled or not and if overrides for a partner are in effect
	- "printCoupon"	Prints the coupon
	- "/tax-offices/extended-tax-services/past-taxes.html" goes to 2nd look review page
	EXAMPLE
		HrbTunneled.buttonAction= {
		"xdefault":{			"url":"/tax-offices/extended-tax-services/past-taxes.html","newWin":false},
		"default":{				"url":"printCoupon","newWin":true},
		"pw_mcm_9423_7764":{	"url":"/includes/partner/bofa-customer/17-1167-EC-CSC---BANK-OF-AMERICA-CUSTOMER-COUPON_1487189484.pdf","newWin":true}
		};
*/

HrbTunneled.buttonAction= {
"xdefault":{				"url":"/tax-offices/extended-tax-services/past-taxes.html","newWin":false},
"default":{				"url":"printCoupon","newWin":true},
"pw_mcm_9423_7764":{	"url":"/tax-offices/extended-tax-services/past-taxes.html","newWin":false},
"123AA11AA22AA33AA44AA55":{"url":"/tax-offices/extended-tax-services/past-taxes.html","newWin":false}
};


HrbTunneled.baseLogoUrl				= "/HRBlock/E-Solutions%20Content/Static%20Files/Image%20Files/ppt_feed/";
HrbTunneled.baseCouponUrl			= /* "/HRBlock/E-Solutions%20Content/Static%20Files/Image%20Files/ppt_feed/"; */
HrbTunneled.couponsEnabled			= true;
HrbTunneled.challengeEnabled		= false;
HrbTunneled.logosEnabled			= false;
HrbTunneled.drawerEnabled			= true;
HrbTunneled.campaignId				= null;
//HrbTunneled.gRecaptchaResponse	= null;
HrbTunneled.partnerInfo				= {};
HrbTunneled.partnerInfo.logoPath	= null;
HrbTunneled.partnerInfo.hasLogo		= false;
HrbTunneled.partnerInfo.couponPath	= null;
HrbTunneled.partnerInfo.hasCoupon	= false;
HrbTunneled.partnerInfo.welcomeText	= null;
HrbTunneled.ctrack					= null;

/*Employer Solutions Drawer in .com*/
var fromMay31 = true;/* Get a Second Look® Review */
var fromSep15 = false;/* Get a Second Look® Review,  Extension Filers */
var fromNov15 = false;/* Get a Second Look® Review, May be EA */
var fromDec15 = false;/* Tax season  */

/*UI Components or Buttons*/
var printCoupon = true; /*Employer Solutions Drawer in .com -  Default - Print my Coupon*/
var secondLook = false;
var extensionFilers = false;
var ea = false; 
var taxSeason = false;

function recaptchaResponse(gRecaptchaResponse){
	//HrbTunneled.gRecaptchaResponse	= gRecaptchaResponse;

	HRBData.digital_data.tool_name		= "partnerCoupon";
	HRBData.digital_data.tool_event		= "recaptchaPassed";
	HRBData.digital_data.tool_client_id	= "hrb";
	HRB.analytics.callRule("tool_event");

	HrbTunneled.enablePrintButton();
	//$("#submitCouponRequest").attr("disabled",false);
}

function recaptchaExpired(){
	HrbTunneled.disablePrintButton();
	//$("#submitCouponRequest").attr("disabled",true);
	grecaptcha.reset();
}

HrbTunneled.enablePrintButton = function(){
		$("#submitCouponRequest").attr("aria-disabled", false);
		$("#submitCouponRequest").attr("disabled",false);
}

HrbTunneled.disablePrintButton = function(){
		//$("#submitCouponRequest").attr("aria-disabled", true);
		//$("#submitCouponRequest").attr("disabled",true);
}

HrbTunneled.submitCouponForm = function(){

	var openInNewWin			= "";
	var goToUrl					= "";
	var bPartnerHasCustomAction	= false;

	if (typeof HrbTunneled.buttonAction[HrbTunneled.campaignId] != "undefined"){
		bPartnerHasCustomAction	= true;
	}

	if (bPartnerHasCustomAction){
		openInNewWin	= HrbTunneled.buttonAction[HrbTunneled.campaignId].newWin;
		goToUrl			= HrbTunneled.buttonAction[HrbTunneled.campaignId].url;

		if (openInNewWin){
			openWin(goToUrl);
		} else {
			document.location=goToUrl;
		}
		return;
	} else if (HrbTunneled.buttonAction["default"].url != "printCoupon" && printCoupon){
		openInNewWin	= HrbTunneled.buttonAction["default"].newWin;
		goToUrl			= HrbTunneled.buttonAction["default"].url;
		
		if (openInNewWin){
			openWin(goToUrl);
		} else {
			document.location=goToUrl;
		}
		return;		
	} else if (HrbTunneled.buttonAction["xdefault"].url != "printCoupon" && secondLook){
		
		openInNewWin	= HrbTunneled.buttonAction["xdefault"].newWin;
		goToUrl			= HrbTunneled.buttonAction["xdefault"].url;

		if (openInNewWin){
			openWin(goToUrl);
		} else {
			document.location=goToUrl;
		}
		return;
	} else {
		if (!(HRB.tools.getQueryUrl("couponInSameWindow")=="true") ){
			$("#idformCoupon").attr("target","myCouponWindow");
			window.open("","myCouponWindow","width=600,height=600,toolbar=0");
		}
		$("#idformCoupon").submit();
	}
}

HrbTunneled.couponPrinted = function(){
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));

	drawerObj.couponPrinted	= true;
/*
	var d = new Date();
    var ms = d.getTime();
	drawerObj.rKeySetAt	= ms;
	drawerObj.rKeyExp	= ms+240000;	key is valid for 5 minutes (300000ms).
	drawerObj.rKey	= HrbTunneled.gRecaptchaResponse;
*/
	HRB.session.storeValue("drawer",JSON.stringify(drawerObj));
	HrbTunneled.disablePrintButton();
	//$("#submitCouponRequest").attr("disabled",true);

/*
	HrbTunneled.setCouponInstructions("See you at the office!");
	HrbTunneled.disablePrintButton();
	// $('#submitCouponRequest').prop('disabled',true);
	$(".recaptchaStep1").css({'visibility':'visible','display':'block'});
	grecaptcha.reset();
	$(".recaptchaStep2").css({'visibility':'hidden','display':'none'});
*/
	HRBData.digital_data.tool_name		= "partnerCoupon";
	HRBData.digital_data.tool_event		= "couponPrinted";
	HRBData.digital_data.tool_client_id	= "hrb";
	HRB.analytics.callRule("tool_event");
};

HrbTunneled.isCouponPrinted = function(){
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));
	if (drawerObj.couponPrinted){
		return true;
	} else {
		return false;
	}
};
/*
HrbTunneled.setCouponInstructions = function(msg){
	$("#couponInstructions").text(msg);
};
*/
HrbTunneled.processPartnerDrawer=function(){

/*
	mega-script.js must have finished main-cookie processing at this point
*/
	fromMay31 = HRB.tools.getQueryUrl("fromMay31") == "true" ? true : false;
	fromSep15 = HRB.tools.getQueryUrl("fromSep15") == "true" ? true : false;
	fromNov15 = HRB.tools.getQueryUrl("fromNov15") == "true" ? true : false;
	fromDec15 = HRB.tools.getQueryUrl("fromDec15") == "true" ? true : false;
	
	if(HRB.tools.getQueryUrl("fromMay31") == null && HRB.tools.getQueryUrl("fromSep15") == null &&
	HRB.tools.getQueryUrl("fromNov15") == null && HRB.tools.getQueryUrl("fromDec15") == null){/*Allow test url with param*/
		fromMay31 = true; /* Enabled  Employer Solutions Drawer :- Get a Second Look® Review */
	}
	
	if(fromMay31){
		printCoupon = false;
		secondLook = true;	
		extensionFilers = true;		
	}
	if(fromSep15){
		printCoupon = false;
		secondLook = true;
		extensionFilers = true;
	}	
	if(fromNov15){
		printCoupon = false;
		secondLook = true;
		ea = true; 
	}		
	if(fromDec15){
		printCoupon = false;
		secondLook = false;
		taxSeason = true;
	}

	HrbTunneled.campaignId	= HRB.tools.getQueryParam("campaignid", HRB.session.getValue("main_cookie"));
	HrbTunneled.ctrack		= HRB.tools.getQueryParam("ctrack",		HRB.session.getValue("main_cookie"));

	if (!HrbTunneled.challengeEnabled){
		HrbTunneled.enablePrintButton();
		//$("#submitCouponRequest").prop("disabled", false);
		$(".recaptchaStep1").css({'visibility':'hidden','display':'none'});
		$("#couponInstructions").css({'visibility':'hidden','display':'none'});
	}

	if (HRB.tools.getQueryUrl("showCoupons")=="true"){
		HrbTunneled.couponsEnabled=true;
	}

	if (HRB.tools.getQueryUrl("showCoupons")=="false"){
		HrbTunneled.couponsEnabled=false;
	}

	if (HrbTunneled.campaignId	==null){
		HrbTunneled.couponsEnabled=false;
	} else {
		$("#campaignId").val(HrbTunneled.campaignId);
	}

	if (HrbTunneled.drawerEnabled)
	{
		/* called by hrb.js after determining which partner will be used and pricing response received */
		/*
		if( (HRB.products.getProducts().getPartnerTunnel()==1 || HRB.bIsSpclEmpSolPartner ) && HRB.page.getPageProperties().getPath() != "/loginRedirect.html" ) 	{
		*/
			
		if( HRB.bIsSpclEmpSolPartner) 	{

			/* Value drawer for this partner */
			HrbTunneled.populateDrawer();

			/* First show in minimized state.  If client has NOT requested it to be minimized, then we expand it. */
			HrbTunneled.showDrawer();

			$('#partner-footer').affix(
			{
				offset: { bottom: $('.partner-footer').offset().bottom }
			});

			/* Determine if drawer should be expanded. */

			/*  If client has minimized the drawer, it should never be shown in expanded state on subsequent pages or this session */
			if (HrbTunneled.clientHasMinimizedTheDrawer()){
				HrbTunneled.minimizeDrawer();
				$('.partnerDrawerTitle span.arrow-icon').addClass('icon-caret-up');
			}
			else{
				/*  show the drawer since client has never minimized it in this session */
				HrbTunneled.expandDrawer();
				$('.partnerDrawerTitle span.arrow-icon').addClass('icon-caret-down');
			}

			$(".partnerDrawerTitle").click(function (){
				$('.partnerDrawerTitle span.arrow-icon').removeClass('icon-caret-down icon-caret-up');
				/* If drawer got click in expanded state, call HrbTunneled.clientClosedTheDrawer(); */
				if(	$('.partnerDrawerContents').css("display")=="block"){
					HrbTunneled.clientClosedTheDrawer();
					$('.partnerDrawerTitle span.arrow-icon').addClass('icon-caret-up');
				}
				else {
					$('.partnerDrawerTitle span.arrow-icon').addClass('icon-caret-down');
				}
				$('.partnerDrawerContents').slideToggle();
			});
		}
	}
};



HrbTunneled.populateDrawer=function(){
/*
	if(g_partner_id == 9314 || g_partner_id == 9311 ) {
		$(".test").hide();
		$(".uber-airbnb").show();
		$(".partnerDrawerCoupon").addClass("airbnb");

	}
	else{
				$(".test").show();

		$(".uber-airbnb").hide();

	}

	if(g_partner_id == 9314){



		$("#airbnb-link").show();
		$("#uber-link").hide();

	   }else if(g_partner_id == 9311){



	   	$("#airbnb-link").hide();
		$("#uber-link").show();

	}
*/
	/*
	The session object "drawer" must be cleared in main framework every time a partner id changes
	Framework script defines this > session:{config:{...,"drawer":{"typ":"STORE","lvl":"root"}
	*/

	if (HrbTunneled.isSessionStorageFound()){
		/* get logo and coupon image information from session storage */
		var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));

		/*
		if(false && HrbTunneled.isCouponPrinted()){
			HrbTunneled.setCouponInstructions("See you at the office!");
			$(".recaptchaStep1").css({'visibility':'hidden','display':'none'});
		} else {
			HrbTunneled.setCouponInstructions("Check the box below to get your coupon.");
			$(".recaptchaStep2").css({'visibility':'hidden','display':'none'});
		}
		*/

		/*  process LOGO */
		HrbTunneled.partnerInfo.hasLogo	= drawerObj.hasLogo;
		HrbTunneled.partnerInfo.logoPath	= drawerObj.logoPath;

		if (HrbTunneled.partnerInfo.hasLogo){
			HrbTunneled.partnerLogoResults(HrbTunneled.partnerInfo.logoPath, true);
		}

		/*  process COUPON */
		HrbTunneled.partnerInfo.hasCoupon	= drawerObj.hasCoupon;
		HrbTunneled.partnerInfo.CouponPath	= drawerObj.couponPath;

		if (HrbTunneled.partnerInfo.hasCoupon){
			HrbTunneled.partnerCouponResults(HrbTunneled.partnerInfo.CouponPath, true);
		}

		/*  process TITLE */
		HrbTunneled.setPartnerWelcomeText();
	}
	else{
		/*  first visit in this session for this partner */
			HrbTunneled.initializeSessionStorage();
		/*  set the LOGO */
			if (HrbTunneled.logosEnabled){
				HrbTunneled.partnerInfo.logoPath	=HrbTunneled.baseLogoUrl+g_partner_id	+"_logo.jpg";
				HRB.tools.IsValidImage(HrbTunneled.partnerInfo.logoPath,	HrbTunneled.partnerLogoResults);
			} else {
				HrbTunneled.partnerLogoResults("",false);
			}
		/*  set the COUPON */
			/*
			HrbTunneled.partnerInfo.couponPath	=HrbTunneled.baseCouponUrl+g_partner_id	+"_coupon.jpg";
			HRB.tools.IsValidImage(HrbTunneled.partnerInfo.couponPath,	HrbTunneled.partnerCouponResults);
			*/
			if (HRB.bIsSpclEmpSolPartner){
				HrbTunneled.partnerCouponResults("",false);
			}
		/* set the title */

			var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));

			$.ajax({
				"type": "GET"
				, "url": "/microservices/empsol/employer/details/"
				, "data": "otpPartnerId="+g_partner_id+"&campaignId="+HrbTunneled.campaignId
				, "dataType": "json"
				, "timeout": 2000
				,beforeSend: function (request){
					request.setRequestHeader("ctrack", HrbTunneled.ctrack);
				}
				, "success":function (data){
					/*
					{
						"promoCode": "12345",
						"partnerId": "9423",
						"campaignId": "AA11AA22AA33AA44AA55",
						"companyDisplayName": "Display TestCompany",
						"vanityUrl": "TestCompany",
						"couponCode": null
					}
					*/
					if(typeof(data.companyDisplayName)!="undefined" && data.companyDisplayName!= null){
						drawerObj.partnerName	= data.companyDisplayName;
						HRB.session.storeValue("drawer",JSON.stringify(drawerObj));
						HrbTunneled.setPartnerWelcomeText();
					}
					else{
						HRB.tools.log("DRAWER: companyDisplayName is undefined or null");
						HrbTunneled.invalidPartner("100");
					}
				},
				error: function (request, status, error) {
					HRB.tools.log("DRAWER: Ajax responseText: "	+ request.responseText);
					HRB.tools.log("DRAWER: Ajax error.message "	+ error.message);
					HRB.tools.log("DRAWER: Ajax error.stack "	+ error.stack);
					HRB.tools.log("DRAWER: Ajax Status "		+ request.status);
					HrbTunneled.invalidPartner("101");
				}
				/*
				,
				complete: function (request, status){
					HRB.session.storeValue("drawer",JSON.stringify(drawerObj));
					HrbTunneled.setPartnerWelcomeText();
				}
				*/
			});
	}
};

HrbTunneled.invalidPartner=function(errCode){
	HRB.tools.log("DRAWER: partner not found");
	HRB.session.storeValue("drawer",null);
	if( HrbTunneled.comDrawer){
		HrbTunneled.setPartnerWelcomeText();
	}else{
		window.location=window.location.pathname+"?otpPartnerId=2631&err=ESOL"+errCode;
	}	
}

HrbTunneled.partnerLogoResults=function(url, imageFound){
	if (imageFound)	{
		HrbTunneled.partnerInfo.hasLogo=true;

		$(".partnerDrawerLogoImage").attr("src",HrbTunneled.baseLogoUrl+g_partner_id+"_logo.jpg");
		/* show logo div */
		$(".partnerDrawerCoBrand").css({"display":"block","visibility":"visible"});
	}
	else {
		HrbTunneled.partnerInfo.logoPath	= null;
		HrbTunneled.partnerInfo.hasLogo	= false;
	}

	/* store LOGO info into session so we don't make this check on every page visited */
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));
	drawerObj.hasLogo	= HrbTunneled.partnerInfo.hasLogo;
	drawerObj.logoPath	= HrbTunneled.partnerInfo.logoPath;

	HRB.session.storeValue("drawer",JSON.stringify(drawerObj));
	HRB.tools.log("DRAWER: partnerLogoResults: store value: "+JSON.stringify(drawerObj));

};


HrbTunneled.partnerCouponResults=function(url, imageFound){
	if (HrbTunneled.couponsEnabled){
		/*
		if (imageFound){
			HrbTunneled.partnerInfo.hasCoupon	= true;

			var newHref			= HrbTunneled.baseCouponUrl+g_partner_id+"_coupon.pdf";
			var newHrefLink1	= "/pdf/Partner-Facing-Guide-FAQ-Combined.pdf";
			var newHrefLink2	= "/pdf/Tax-Information-for-Hosts1-4-16.pdf";

			$(".partnerDrawerCouponLink").attr("href",newHref);
			$(".partnerDrawerCouponLink").attr("href",newHref);

			$(".partnerDrawerCoupon").css({"display":"block","visibility":"visible"});

			$("#partner-link1").attr("href",newHrefLink1);
			$("#partner-link2").attr("href",newHrefLink2);
		}
		else */
		if (HRB.bIsSpclEmpSolPartner){
			/* all special emp partners have coupons */
			HrbTunneled.partnerInfo.hasCoupon	= true;
			/* show coupon div */
			$(".partnerDrawerCoupon").css({"display":"block","visibility":"visible"});
		}
	}
	else {
		HrbTunneled.partnerInfo.couponPath	= null;
		HrbTunneled.partnerInfo.hasCoupon	= false;
	}

	/* store COUPON info into session so we don't make this check on every page visited */
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));
	drawerObj.hasCoupon		= HrbTunneled.partnerInfo.hasCoupon;
	drawerObj.couponPath	= HrbTunneled.partnerInfo.couponPath;
	HRB.session.storeValue("drawer",JSON.stringify(drawerObj));
	HRB.tools.log("DRAWER: partnerCouponResults: store value: "+JSON.stringify(drawerObj));
};

HrbTunneled.initializeSessionStorage=function(){
	var drawerObj	= {"closed":false,"hasLogo":false,"logoPath":"","hasCoupon":false,"couponPath":"","couponPrinted":false,"partnerName":""};
	HRB.session.storeValue("drawer",JSON.stringify(drawerObj));
	HRB.tools.log("DRAWER: initializeSessionStorage store value: "+JSON.stringify(drawerObj));
};

HrbTunneled.setPartnerWelcomeText=function(){
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));
	if(drawerObj){
		$(".partnerDrawerTitle .partnerWelcome").html("Welcome "+drawerObj.partnerName);
	}	
	
	$(".partnerDrawerCoupon").css({"display":"block","visibility":"visible"});		
	$('.partnerDrawerPod.partner').removeClass('two-cols');
	$('.partnerDrawerPod .partnerCouponContents').removeClass('half-width');	 
	$('.partnerDrawerPod .partnerCouponContents.extension-filers').addClass('hide');
	$('.partnerDrawerPod .partnerCouponContents.ea').addClass('hide');	
	$('.partnerDrawerPod .partnerCouponContents.tax-season').addClass('hide');
	if(extensionFilers){/*Show Extension Filer*/ 
		$('.partnerDrawerPod.partner').addClass('two-cols');
		$('.partnerDrawerPod .partnerCouponContents').addClass('half-width');
		$('.partnerDrawerPod .partnerCouponContents.extension-filers').removeClass('hide');
	}			
	if(ea){/*Show EA*/ 
		$('.partnerDrawerPod.partner').addClass('two-cols');
		$('.partnerDrawerPod .partnerCouponContents').addClass('half-width');
		$('.partnerDrawerPod .partnerCouponContents.ea').removeClass('hide');
	}			
	if(taxSeason){/*Show Tax Season*/ 		
		$('.partnerDrawerPod .partnerCouponContents.print-coupon').addClass('hide');
		$('.partnerDrawerPod .partnerCouponContents.tax-season').removeClass('hide');
	}	
		
	HrbTunneled.setButtonTitle();
	HrbTunneled.setButtonText();	
};

HrbTunneled.setButtonTitle=function(){
	// if campaign is found in HrbTunneled.buttonTitleCustom use that value otherwise use HrbTunneled.buttonTitleDefault
	var buttonTitle	= HrbTunneled.buttonTitle[HrbTunneled.campaignId];
	if (typeof buttonTitle != "undefined"){
		$("#buttonTitle").html(buttonTitle);
	} else {
		$("#buttonTitle").html(HrbTunneled.buttonTitle["default"]);		
		
		if(secondLook){/*show Second Look*/
			$("#buttonTitle").html(HrbTunneled.buttonTitle["xdefault"]);
		}else {
			$("#buttonTitle").html(HrbTunneled.buttonTitle["default"]);
		}
	}
}

HrbTunneled.setButtonText=function(){
	// if campaign is found in HrbTunneled.buttonTextCustom use that value otherwise use HrbTunneled.buttonTextDefault
	var buttonText	= HrbTunneled.buttonText[HrbTunneled.campaignId];
	if (typeof buttonText != "undefined"){
		// partner specific text found
		$("#submitCouponRequest").attr("value",buttonText);
	} else {
		// use default text
		$("#submitCouponRequest").attr("value",HrbTunneled.buttonText["default"]);
		if(secondLook){/*show Second Look*/
			$("#submitCouponRequest").attr("value",HrbTunneled.buttonText["xdefault"]);
		}else{
			$("#submitCouponRequest").attr("value",HrbTunneled.buttonText["default"]);
		} 
	}
}

HrbTunneled.clientHasMinimizedTheDrawer=function(){
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));

	if (drawerObj!=null && drawerObj.closed!=null && drawerObj.closed){
		return true;
	}
	else {
		return false;
	}
};

HrbTunneled.clientClosedTheDrawer=function(){
	/*
	When client clicks the drawer to HIDE it, call this function.
	Write session storage value to know NOT to expand the drawer when visiting subsequent pages
	*/
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));
	if (drawerObj!=null){
		drawerObj.closed=true;
		HRB.session.storeValue("drawer",JSON.stringify(drawerObj));
	}
	
};

HrbTunneled.isSessionStorageFound=function(){
	var drawerObj = jQuery.parseJSON(HRB.session.getValue("drawer"));

	if (drawerObj==null){
		HRB.tools.log("DRAWER: isSessionStorageFound(): NOT FOUND");
		return false;
	}
	else {
		HRB.tools.log("DRAWER: isSessionStorageFound(): FOUND");
		return true;
	}
};

HrbTunneled.expandDrawer=function(){
	/*$('.partnerDrawerContents').css({"display":"block"}); */
	$('.partnerDrawerContents').slideDown();
};

HrbTunneled.minimizeDrawer=function(){
	/*$('.partnerDrawerContents').css({"display":"none"}); */
	$('.partnerDrawerContents').slideUp();
};

HrbTunneled.showDrawer=function(){
	$('.partnerDrawerContents').slideUp();
	$(".partner-footer").css({"display":"block","visibility":"visible"});
};
</script>

