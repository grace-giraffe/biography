<script type="text/javascript">
function scrollWhenLoaded($scrollToObj) {
    /* page-loaded class is added by init.js
       if we don't wait for load, it can scroll up and down */
    var pgIsLoaded = $('.page-loaded').length > 0;
    if (pgIsLoaded) {

        $('html,body').animate({ scrollTop: $scrollToObj.offset().top - 60 }, 500);
    } else {
        /* or wait until load event */
        $(window).load(function() {
            $('html,body').animate({ scrollTop: $scrollToObj.offset().top - 60 }, 500);
        });
    }
}


function clearProductSelector() {
    $('.ct-op-selector input[type=checkbox]').prop('checked', false);
}


$(document).ready(function() {

    var resizeTimer;
    $(window).on('resize', function(e) {
        clearTimeout(resizeTimer);
        var isMobile = Modernizr.mq('(max-width: 47.9375em)');

        if (!isMobile) {
            resizeTimer = setTimeout(function() {
                $('.ct-home__content').removeClass('open');
            }, 350);
        }
    });



    /* BUTTONS IN HERO THAT OPEN APPS ====================== */

    /* File Free Online button */
    $('.js-open-ct-online').on('click', function() {
        $('.ct-home-entry, .ct-combo-entry, #tpf-tool, .ct-op-results, .tpr-screen-results, #tpg-flyout-section, .ct-tpr').hide();
        $('.ct-op').removeClass('hide');
        $('.ct-op-selector').removeClass('op-tpr').slideDown();
        $('.ct-op > .ct-header').addClass('show-restart');
        clearProductSelector();

        /* function from tool pod below */
        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);

        /* virtual page load - online product selector */
        HRBData.digital_data.virtual_page_name = "online-product-selector";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "hero-online-btn";
        HRB.analytics.callRule("vpage_global");
    });

    /* Half Off button */
    $('.js-open-ct-half-off').on('click', function() {
        $('.ct-home-entry, #tpf-tool, .ct-op-selector, .ct-op-results, .tpr-screen-results, #tpg-flyout-section, .ct-tpr').hide();
		$('.ct-op').removeClass('hide');
        $('.ct-combo-entry').addClass('ct-show-half-off').slideDown();
        $('.ct-op > .ct-header').addClass('show-restart');

        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);

        /* virtual page load - retail entry screen */
        HRBData.digital_data.virtual_page_name = "retail-guided-entry";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "hero-half-off-btn";
        HRB.analytics.callRule("vpage_global");
    });

    /* RA button */
    $('.js-open-ct-ra').on('click', function() {
        $('.ct-home-entry, #tpf-tool, .ct-op-selector, .ct-op-results, .tpr-screen-results, #tpg-flyout-section, .ct-tpr').hide();
        $('.ct-op').removeClass('hide');
        $('.ct-combo-entry').removeClass('ct-show-ez').addClass('ct-show-ra').slideDown();
        $('.ct-op > .ct-header').addClass('show-restart');

        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);

        /* virtual page load - retail entry screen */
        HRBData.digital_data.virtual_page_name = "ra-entry";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "hero-ra-btn";
        HRB.analytics.callRule("vpage_global");
    });

    /* EZ button */
    $('.js-open-ct-ez').on('click', function() {
        $('.ct-home-entry, #tpf-tool, .ct-op-selector, .ct-op-results, .tpr-screen-results, #tpg-flyout-section, .ct-tpr').hide();
        $('.ct-op').removeClass('hide');
        $('.ct-combo-entry').removeClass('ct-show-ra').addClass('ct-show-ez').slideDown();
        $('.ct-op > .ct-header').addClass('show-restart');

        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);

        /* virtual page load - retail entry screen */
        HRBData.digital_data.virtual_page_name = "ez-entry";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "hero-ez-btn";
        HRB.analytics.callRule("vpage_global");
    });
    
    /* Find an Office button */
    $('.js-open-ct-office').on('click', function() {
        $('.ct-home-entry, #tpf-tool, .ct-op-selector, .ct-op-results, .tpr-screen-results, #tpg-flyout-section, .ct-tpr').hide();
        $('.ct-op').removeClass('hide');
        $('.ct-combo-entry').slideDown();
        $('.ct-op > .ct-header').addClass('show-restart');

        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);
        
        /* virtual page load - retail entry screen */
        HRBData.digital_data.virtual_page_name = "retail-guided-entry";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "hero-find-office-btn";
        HRB.analytics.callRule("vpage_global");
    });



    /* BUTTONS IN ENTRY SCREEN THAT OPEN APPS ====================== */

    /* Mobile H3 headings act as buttons */
    $('.ct-home__content h3').on('click', function() {
        //var isMobile = Modernizr.mq('(max-width: 40.9375em)');
        var isMobile = Modernizr.mq('(max-width: 47.9375em)');
        if (isMobile) {

            var isOpen = $('.ct-home__content.open').length > 0;
            if (isOpen) {
                var $scrollToObj = $('section.ct-op');
                $('.ct-home__content').removeClass('open');
            } else {
                var $scrollToObj = $(this);
                $('.ct-home__content').addClass('open');
            }
            scrollWhenLoaded($scrollToObj);

        } else {
            $('.ct-home__content').removeClass('open');
        }
    });

    /* Online button */
    $('.ct-home-entry__btn-online').on('click', function() {
        $(this).parents('.ct-home-entry').hide();
        $('.ct-op-selector').removeClass('op-tpr').slideDown();
        $('.ct-op > .ct-header').addClass('show-restart');
        clearProductSelector();

        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);

        /* virtual page load - online product selector */
        HRBData.digital_data.virtual_page_name = "online-product-selector";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "entry-online-btn";
        HRB.analytics.callRule("vpage_global");
    });

    /* Online + Tax Pro Review button */
    $('.ct-home-entry__btn-tpr').on('click', function() {
        $(this).parents('.ct-home-entry').hide();
        $('.ct-tpr').slideDown();
        $('.ct-op-selector').addClass('op-tpr');
        $('.ct-op > .ct-header').addClass('show-restart');
        clearProductSelector();

        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);

        /* virtual page load - online product selector */
        HRBData.digital_data.virtual_page_name = "tpr 3 easy steps";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "entry-tpr-btn";
        HRB.analytics.callRule("vpage_global");
    });

   
/* Tax Pro Go button */
//Conversion tool funnel removed for 2018 Post Season, button will go directly to TPG page
  // $('.ct-home-entry__btn-tpg').on('click', function() {
  //     $(this).parents('.ct-home-entry').hide();
  //     $('#tpg-flyout-section').slideDown();
  //     $('.ct-op > .ct-header').addClass('show-restart');
	// 
  //     var $tool = $('.ct-op');
  //     scrollWhenLoaded($tool);
	// 
  //     /* virtual page load - online product selector */
  //     HRBData.digital_data.virtual_page_name = "tpg-tool";
  //     HRBData.digital_data.loc = "conversion-tool";
  //     HRBData.digital_data.nm = "entry-tpg-btn";
  //     HRB.analytics.callRule("vpage_global");
  // });

    /* Office appointment button */
    $('.ct-home-entry__btn-office').on('click', function() {
        $(this).parents('.ct-home-entry').hide();
        $('.ct-combo-entry').removeClass('ct-show-ez ct-show-ra ct-show-half-off').slideDown();
        $('.ct-op > .ct-header').addClass('show-restart');

        var $tool = $('.ct-op');
        scrollWhenLoaded($tool);

        /* virtual page load - retail entry screen */
        HRBData.digital_data.virtual_page_name = "retail-guided-entry";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "entry-retail-btn";
        HRB.analytics.callRule("vpage_global");
    });


    /* BUTTON IN TPR SCREEN THAT OPENS PRODUCT SELECTOR ================= */
    $('.ct-tpr__open-selector').on('click', function(){
        $('.ct-tpr').slideUp();
        $('.ct-op-selector').slideDown();
        
        /* virtual page load - online product selector */
        HRBData.digital_data.virtual_page_name = "tpr online product selector";
        HRBData.digital_data.loc = "conversion-tool";
        HRBData.digital_data.nm = "choose online product btn";
        HRB.analytics.callRule("vpage_global");
    });

    /* BUTTONS IN TPR RESULTS SCREENS THAT OPEN/CLOSE SYSTEM REQUIREMENTS */

    /* Open requirements - large screens */
    $('.btn-requirements').on('click', function() {
        $('#requirements').slideDown();
        $(this).attr('aria-expanded', 'true');
    });

    /* Close requirements - large screens */
    $('.js-close-requirement').on('click', function() {
        $('#requirements').slideUp();
        $('.btn-requirements, .btn-requirements-sm-only').attr('aria-expanded', 'false');
    });

    /* Open/close requirements - small screens */
    $('.btn-requirements-sm-only').on('click', function() {
        var isExpanded = $(this).attr('aria-expanded');
        if (isExpanded == 'true') {
            $('#requirements').slideUp();
            $(this).attr('aria-expanded', 'false');
        } else {
            $('#requirements').slideDown();
            $(this).attr('aria-expanded', 'true');
        }
    });


});
</script>
<!-- LOGIC and BUTTONS FOR ONLINE PRODUCT SELECTOR -->
<script type="text/javascript">
$(document).ready(function() {
    /* Determine Messaging to use during specific periods */

    var todayDate = new Date();
    var todayMS = todayDate.getTime();

    // For testing you can value forceDate with MM/DD/YYYY
    var forceDate = HRB.tools.getQueryUrl("forceDate");

    if (forceDate != null) {
        var numbers = forceDate.match(/\d+/g);
        var date = new Date(numbers[2], numbers[0] - 1, numbers[1]);
        todayMS = date.getTime();
    }

    var inMoreZeroPromoPeriod = "",
        rp = {};


    var onlinePromoCheck = function() {
        // Online Period 1 - 12/1/2017 to 3/31/2018
        // Show MORE ZERO instead of Online Deluxe

        // ONLINE PROMO PERIODS
        var onlineMoreZeroBegin = "12/01/2017";
        var onlineMoreZeroEnd = "03/31/2018";

        var onlineMoreZeroBeginMS = Date.parse(onlineMoreZeroBegin);
        var onlineMoreZeroEndMS = Date.parse(onlineMoreZeroEnd);

        // DETERMINE ONLINE MESSAGING
        inMoreZeroPromoPeriod = false;

        if (todayMS >= onlineMoreZeroBeginMS && todayMS <= onlineMoreZeroEndMS) {
            inMoreZeroPromoPeriod = true;
        }

    }

    var setTPRSession = function(productID, activeState) {
        var tprSelections = HRB.session.getValue("tprSelections");
        var tprSelectionsObj = jQuery.parseJSON(tprSelections);

        if (productID !== null) {
            tprSelectionsObj[productID] = activeState;
            HRB.session.storeValue("tprSelections", JSON.stringify(tprSelectionsObj));
            HRB.ui.setMDLTprSelections();
            HRB.products.displayPricing();
        }
    }

    var TPR_PRODUCTS_ID = {
        FREE: 201,
        DELUXE: 204,
        PREMIUM: 32,
        SELFEMPLOYED: 47
    }

    var getProductID = function(productName) {
        var productIDProp = productName.toUpperCase().replace(/-/g, "");

        return TPR_PRODUCTS_ID[productIDProp];
    }

    var getRecommendedProduct = function() {
        rp.prodName = "free";

        if ($('.ct-op input[name=haveChildren]').is(':checked')) {
            rp.prodName = "free";
        }

        /* During More Zero promo period,
           recommend Free product for ownHome instead of Deluxe */
        onlinePromoCheck();

        if ($('.ct-op input[name=ownHome]').is(':checked')) {
            if (inMoreZeroPromoPeriod) {
                rp.prodName = "free";
            } else {
                rp.prodName = "deluxe";
            }
        }
        if ($('.ct-op input[name=haveHsa]').is(':checked')) {
            rp.prodName = "deluxe";
        }
        if ($('.ct-op input[name=contractor]').is(':checked') ||
            $('.ct-op input[name=haveInvestments]').is(':checked') ||
            $('.ct-op input[name=ownRentalProperty]').is(':checked')) {
            rp.prodName = "premium";
        }
        if ($('.ct-op input[name=selfEmployed]').is(':checked') ||
            $('.ct-op input[name=businessPartnership]').is(':checked')) {
            rp.prodName = "self-employed";
        }

        if (rp.prodName != "") {
            //alert('rp.prodName: ' + rp.prodName);
        }
    }

    /* check dates and get product on page load */
    onlinePromoCheck();
    getRecommendedProduct();

    $('.ct-op input[type=checkbox]').on('change', function() {
        getRecommendedProduct();
    });

    /* button clicks */
    $('.ct-op .ct-restart--main, .ct-op .ct-prev').on('click', function() {
        /* Clear TPR selection from session storage */
        setTPRSession(getProductID(rp.prodName), "N");

        $('.ct-op-selector input[type=checkbox]').prop('checked', false);
        getRecommendedProduct();
        $('#tpf-tool, .ct-combo-entry, .ct-op-selector, .ct-op-results, .tpr-screen-results, #tpg-flyout-section, .ct-tpr').hide();
        $('.ct-op > .ct-header').removeClass('show-restart');
        $('.ct-home-entry').slideDown();
        /* remove style attr so it doesn't affect small/mobile layout if resized down */
        $('.ct-home-entry').removeAttr('style');
    });

    $('.ct-next').on('click', function() {
        var isTprProduct = $('.ct-op-selector.op-tpr').length > 0;
        if (isTprProduct) {
            var rpId = ".tpr-screen-results-" + rp.prodName;
            // hide the selector
            $('.ct-op-selector').hide();
            // hide all forms of results screens
            $('[class*="tpr-screen-results-"]').hide();
            //$('.ct-op-results').hide();
            // show the correct TPR results screen
            $('.tpr-screen-results,' + rpId).show();
            // set session storage
            setTPRSession(getProductID(rp.prodName), "Y");

            /* virtual page load - online product results */
            HRBData.digital_data.virtual_page_name = "tpr-product-recommendation";
            HRBData.digital_data.loc = "conversion-tool";
            HRBData.digital_data.nm = "next-btn";
            HRB.analytics.callRule("vpage_global");
        } else {
            var rpId = "#ct-op-results__" + rp.prodName;
            // hide the selector
            $('.ct-op-selector').hide();
            // hide all forms of results screens
            //$('.tpr-screen-results').hide();
            $('[id^="ct-op-results__"]').hide();
            // show the correct results screen
            $('.ct-op-results,' + rpId).show();

            /* virtual page load - online product results */
            HRBData.digital_data.virtual_page_name = "online-product-recommendation";
            HRBData.digital_data.loc = "conversion-tool";
            HRBData.digital_data.nm = "next-btn";
            HRB.analytics.callRule("vpage_global");
        }
    });
});
</script>
<!-- FOR TPG APP -->
<script type="text/javascript">
function initAngular(app_name) {
    var _el = "[data-application='" + app_name + "']";
    var dom_el = $(_el).get(0);
    var rootscope = angular.element(dom_el).scope();

    if (rootscope) {
        $('.tpg-tool-section').slideDown().hide();
        $('#tpg-view').show();
    } else {
        angular.element(function() {
            angular.bootstrap(dom_el, [app_name]);
        });
    }
}
$(document).ready(function() {
    $(document).on('click', '.startang', function(){
        var $tpgTool = $('.ct-op');
        initAngular('seg2');
        $('#tpg-popup').css('display','none');
        $('.tpg-tool-section').slideUp();
        $('#tpg-view').slideDown().removeClass('hiddenTrue');
        setTimeout(function() {
            $('.ct-op #tpg-view .ct-restart').css('visibility', 'visible');
            $('.ct-restart--main').hide();
        }, 500);
        scrollWhenLoaded($tpgTool);


        var dtmRuleToCall = "tool_event";
        try {
            HRBData.digital_data.tool_name = "seg2";
            HRBData.digital_data.tool_step = "";
            HRBData.digital_data.tool_event = "tpg_screener_start";
            HRBData.digital_data.tool_lang = "en";
            HRBData.digital_data.tool_client_id = "hrb";
            console.log("on get started", dtmRuleToCall);
            _satellite.track(dtmRuleToCall);

        } catch (e) {
            console.log("Error: DTM call '" + dtmRuleToCall + "' failed : " + e);
        }
        var dtmRuleToCall = "tool_step";
        try {
            HRBData.digital_data.tool_name = "seg2";
            HRBData.digital_data.tool_step = "lifestyle";
            HRBData.digital_data.tool_event = "";
            HRBData.digital_data.tool_lang = "en";
            HRBData.digital_data.tool_client_id = "hrb";
            console.log("Omniture for Seg2 on lifestyle");

            _satellite.track(dtmRuleToCall);

        } catch (e) {
            console.log("Error: DTM call '" + dtmRuleToCall + "' failed : " + e);
        }

    });
    $('#angular-app-start').on('click', function() {
        $('#tpg-view').slideUp().hide().addClass('hiddenTrue');
        $('.tpg-tool-section').slideDown();
        $('#resetForm').trigger('click');
        // we hid this restart when we went into TPG, so remove the display:none
        $('.ct-restart--main').removeAttr('style');
    });

    $('.error-btn').on('click', function() {
        $('#tpg-view').slideDown();
        $('.tpg-tool-section').slideUp().show();
        $('#tpg-view').hide();

    });
});
</script>
<script type="text/javascript">
$(document).ready(function() {

    function updateCaption() {
        var caption = 'Make A Tax Appointment';
        if (tpfType == "IFL") {
            caption = 'Make An RA Appointment';
        } else if (tpfType == "1040ez") {
            caption = 'Make An EZ Appointment';
        }else if (tpfType == "HALFOFF") {
            caption = 'Make An Appointment';
        }
        caption = 'Make A Tax Appointment';
        $('.ct-caption').text(caption);
        $('.ct-guided-entry.entry').data('caption', caption);
    }
    /*TPF Tool Filter Check box*/
    /**Add any type of filters like DEFAULT, EAP, EAFF , 1040ez, IFL etc **/
    function updateFilter(obj) {
        var group = ".entry .ct-checkboxes input:checkbox[name='" + obj.attr("name") + "']";
        $(group).attr("checked", false);
        obj.attr("checked", true);
        try {
            componentHandler.upgradeDom();
            document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.tax').MaterialCheckbox.uncheck();
			//document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.halfoff').MaterialCheckbox.uncheck();
            //document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.ra').MaterialCheckbox.uncheck();
            //document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.ez').MaterialCheckbox.uncheck();
            document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.' + obj.attr("id")).MaterialCheckbox.check();
        } catch (e) {
            console.log("Material UI checkbox failed : " + e);
        }
       if (obj.attr("id") == "ez") {
            tpfType = "1040ez";
        } else if (obj.attr("id") == "ra") {
            tpfType = "IFL";
        }else if (obj.attr("id") == "halfoff") {
            tpfType = "HALFOFF";
        } else {
            tpfType = "DEFAULT";
        }
        updateCaption();
    }

    $(".entry .ct-checkboxes .mdl-checkbox__input").on('click', function(e) {
        var obj = $(this);
        if (obj.is(":checked")) {
            updateFilter(obj);
        } else {
            var group = ".entry .ct-checkboxes input:checkbox[name='" + obj.attr("name") + "']";
            $(group).attr("checked", false);
            try {
                componentHandler.upgradeDom();
                $('.entry .ct-checkboxes .mdl-js-checkbox.tax').attr("checked", true);
                document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.tax').MaterialCheckbox.check();
				//document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.halfoff').MaterialCheckbox.uncheck();
                //document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.ra').MaterialCheckbox.uncheck();
                //document.querySelector('.entry .ct-checkboxes .mdl-js-checkbox.ez').MaterialCheckbox.uncheck();
            } catch (e) {
                console.log("Material UI checkbox failed : " + e);
            }
            tpfType = "DEFAULT";
            updateCaption();
        }
        console.log('You have chosen : ' + tpfType);
    });


    $('.appt-filter').on('click', function(e) {
        $('.entry .ct-checkboxes').addClass('hide');
        var filterType = $(this).data('filter-type');
        if (filterType == 'default') {
            $('.entry .ct-checkboxes').removeClass('hide');
            updateFilter($(".entry .ct-checkboxes #tax"));
        } else if (filterType == 'ifl') {
            updateFilter($(".entry .ct-checkboxes #ra"));
        } else if (filterType == 'ez') {
            updateFilter($(".entry .ct-checkboxes #ez"));
        }else if (filterType == 'halfoff') {
            updateFilter($(".entry .ct-checkboxes #halfoff"));
        }
    });

    /*TPF Tool Filter Default Value
    if ($("input:checkbox[id='tax']").is(':checked')) {
        tpfType = "DEFAULT";
    }else if ($("input:checkbox[id='ra']").is(':checked')) {
        tpfType = "IFL";
    }else if ($("input:checkbox[id='ez']").is(':checked')) {
        tpfType = "1040ez";
    }
    updateCaption();*/
});
/*Switch To TPF Tool App*/
/**Implement the css or script while switching to APP**/
function switchToApp() {
    $('.ct-op').addClass('hide');
}

/*Back To Entry Screen*/
/**Implement the css or script while back to Entry screen**/
function switchToEntry() {
    $('.ct-op').removeClass('hide');
}
</script>

