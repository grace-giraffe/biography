<script type="text/javascript">
function showTprPopup(){
        $('html').addClass('show-tprp');
        //alert('HRB.productForTprPrompt: ' + HRB.productForTprPrompt);
        $('#tpr-popup').removeAttr('class').addClass('show-' + HRB.productForTprPrompt);
        $('#tpr-popup .tprp').focus();
    }

    $(document).ready(function(){

        /* Trap TAB & Shift TAB inside popup */
        var $popup = $('.tprp'),
            $lastBtn = $('.trpr__last-btn');

        $popup.on('keydown', function(e) {
            if ( (e.keyCode || e.which) == 9) {
                if ( e.target.nodeName === "DIV" && e.shiftKey){
                    e.preventDefault();
                    $lastBtn.focus();
                }

                if ( $(e.target).hasClass('trpr__last-btn') && !e.shiftKey ){
                    e.preventDefault();
                    $popup.focus();
                }
            }
        });
    });
</script>

