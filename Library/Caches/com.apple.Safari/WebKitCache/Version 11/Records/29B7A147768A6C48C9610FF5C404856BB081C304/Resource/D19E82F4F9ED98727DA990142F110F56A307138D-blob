<script type="text/javascript">
	$(document).ready(function () {
		SearchFindADoc4();
	});

	function SearchFindADoc4() {
		$(".search-fad4-input").autocomplete({
			minLength: 2,
			source: function (request, response) {
				$.ajax({
					type: "POST",
					contentType: "application/json; charset=utf-8",
					url: "/Default.aspx/GetAutoCompleteData",
					data: "{'searchText':'" + $('.search-fad4-input').val() + "', 'bPhysicianOnly':'True'}",
					dataType: "json",
					success: function (data) {
						response(data.d);
					},
					error: function (result) {
						/*(alert("Error");*/
					}
				});
            },
            select: function (e, ui) {
                //Doctor Name
                var searchText = ui.item.value;
                window.location.href = "/find-a-doctor/find-a-doctor?type=1&search=" + searchText;
            }
		});
	}
</script>



