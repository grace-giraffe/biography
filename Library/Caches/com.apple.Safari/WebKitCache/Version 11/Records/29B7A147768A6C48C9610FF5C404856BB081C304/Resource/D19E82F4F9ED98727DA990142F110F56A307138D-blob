<script>
    $(document).ready(function () {

        //If autocomplate list is open, change delay to give user time to get back to dropdown
        //before it closes
        $("nav .form-control.doctor-search").on("autocompleteopen", function (event, ui) {
            $(this).data("delay", "5000");
        });

        //Let's us set delay back to default when option has been selected
        $("nav .form-control.doctor-search").on("autocompleteselect", function (event, ui) {
            $(this).data("delay", "0");
        });

        //If select is open, change delay to give user time to get back to dropdown
        //before it closes
        $("#ctl00_header_ddlFADFlyoutSpecialty").change(function () {
            $("nav .form-control.doctor-search").data("delay", "2000");
        });

    });

</script>

