            {
                var _timeTracker = new progTimer.TrackTiming("PageLoadTime");
                _timeTracker._startTime(startTm);
                _timeTracker._endTime(new Date().getTime());
                _timeTracker._track();                
            }
        }
        catch (e) { }

            
                if (typeof _gaObj != 'undefined' && _gaObj instanceof Object){
                    _gaObj.GATrackEvent("progCom", "encryptionType", 'TLSv1.2');
                }
            

    };            
</script>
