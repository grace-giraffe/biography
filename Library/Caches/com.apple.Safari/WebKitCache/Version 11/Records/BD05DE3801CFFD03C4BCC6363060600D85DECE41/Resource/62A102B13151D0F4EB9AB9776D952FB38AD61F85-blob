		<script language="JavaScript" type="text/javascript" src="https://www.progressive.com/content/js/domainprogressive/pgrx-baseline.min.js"></script>
		<script language="JavaScript" type="text/javascript" src="https://www.progressive.com/content/js/domainprogressive/gaEventTracking-baseline.min.js"></script>
		<script language="JavaScript" type="text/javascript" src="https://www.progressive.com/content/js/domainprogressive/accessibility.min.js"></script>
		<script language="JavaScript" type="text/javascript" src="https://www.progressive.com/content/js/notranslate.min.js"></script>

		<script type="text/javascript">
		/* Loading options / Spinner
		/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
		/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */
		var loadingOptions = {
			lockForm: function (form) {
				$(form).find(":input").attr('disabled', '');
				$(form).find(":input").blur();
			},
			unlockForm: function (form) {
				$(form).find(":input").removeAttr('disabled');
			},
			spinner: {
				init: function () {
					$("form").find(".button").append("<div class='spinner'><p>Loading</p><ul><li></li><li></li><li></li></ul></div>");
				},
				start: function (form) {
					$button = $(form).find(".button").addClass("waiting");

					return function () {
						setTimeout(function () {
							$button.removeClass("waiting");
						}, 5000);
					}
				}
			},
			disable: {
				start: function (form) {
					var $button = $(form).find(".button");
					var buttonInput = $button.find("input[type=button],input[type=submit]");
					buttonInput.prop("disabled", true);
					$button.addClass("waiting");

					setTimeout(function () {
						$button.removeClass("waiting");
						buttonInput.prop("disabled", false);
					}, 5000);
				}
			},
			defaultOption: null
		};
		loadingOptions.defaultOption = loadingOptions.spinner.start;

		$(window).load(function () {
			loadingOptions.spinner.init();
		});
		</script>


		<script type="text/javascript">
		$(window).bind("load", function () {
			var element = document.createElement("script");
			element.src = "https://preferences.truste.com/webservices/js?domain=progressive.com&type=progressive&js=2";
			$("body").append(element);
		});
		</script>


		
		
		 
