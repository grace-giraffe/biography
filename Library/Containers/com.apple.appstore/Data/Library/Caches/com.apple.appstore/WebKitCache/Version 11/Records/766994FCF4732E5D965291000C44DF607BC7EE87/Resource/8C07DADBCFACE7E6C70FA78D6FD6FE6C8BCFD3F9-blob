            pageMetricsUrl += (hasitcCt) ? "%26" : "&cookie=";
            pageMetricsUrl += "itscc%3D" + itscc;
        }
    }
    var containingDiv = document.createElement('div');
    containingDiv.innerHTML = "<img width='0' height='0' border='0' alt='' aria-hidden=true src='" + pageMetricsUrl + "'/>";
    document.body.appendChild(containingDiv);

